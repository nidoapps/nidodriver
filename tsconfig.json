{
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "components": ["components/*"],
      "store": ["store/*"],
      "screens": ["screens/*"],
      "utils": ["utils/*"],
      "constants/*": ["constants/*"],
      "assets/*": ["assets/*"],
      "models/*": ["models/*"],
      "hooks": ["hooks/*"],
      "mocks": ["mocks/*"]
    }
  },
  "extends": "expo/tsconfig.base"
}
